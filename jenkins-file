 properties([pipelineTriggers([githubPush()])])
 
 pipeline {
    /* specify nodes for executing */
    agent any
   tools {nodejs "node"}
    stages {
        
         stage('Web Page Test') {
            steps {
                bat 'npm i webpagetest'
                bat 'webpagetest test https://www.gmail.com/search?q=mac+m1+chip --poll --wait --key %APIKEY% --first'
            }
        }
    }
}
