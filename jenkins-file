 properties([pipelineTriggers([githubPush()])])
 
 pipeline {
    /* specify nodes for executing */
    agent any
   tools {nodejs "node"}
    stages {
        
         stage('Web Page Test') {
            steps {
                bat 'npm i webpagetest -g'
                bat 'webpagetest test https://www.google.com/search?q=mac+m1+chip --poll --wait --key %APIKEY% --first'
                junit skipPublishingChecks: true, testResults: 'git/test-reports/test.xml'
            }
        }
    }
}
